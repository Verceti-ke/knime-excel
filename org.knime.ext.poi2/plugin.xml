     <extension
           point="org.knime.workbench.repository.registeredFileExtensions">
        <FileExtensionMapping
              NodeFactory="org.knime.ext.poi2.node.read4.XLSReaderNodeFactory">
           <FileExtension
                 extension=".xls">
           </FileExtension>
           <FileExtension
                 extension=".xlsx">
           </FileExtension>
        </FileExtensionMapping>
    </extension>
</plugin>
