<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="false">
    <fileset name="Java Files" enabled="true" check-config-name="UniKN KNIME" local="false">
        <file-match-pattern match-pattern=".*\.java$" include-pattern="true"/>
        <file-match-pattern match-pattern=".*Test\.java$" include-pattern="false"/>
    </fileset>
    <fileset name="Unit Test Files" enabled="true" check-config-name="UniKN KNIME Unittest" local="false">
        <file-match-pattern match-pattern=".*Test\.java$" include-pattern="true"/>
    </fileset>
    <filter name="NonSrcDirs" enabled="true"/>
</fileset-config>
